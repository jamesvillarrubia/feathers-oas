{
  "name": "feathers-oas",
  "version": "0.0.0-releaseit",
  "description": "An openapi generator for FeathersJS v5",
  "main": "bin/bin.js",
  "types": "lib/",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "bin": {
    "foas": "./bin/bin.js"
  },
  "type": "module",
  "peerDependencies": {
    "@feathersjs/errors": "^5.0.0-pre.38 || ^4.5.12"
  },
  "dependencies": {
    "cosmiconfig": "^8.1.0",
    "debug": "^4.3.4",
    "npm-add-script": "^1.1.0",
    "pluralize": "^8.0.0",
    "swagger-merger": "^1.5.4",
    "swagger-to-static": "^1.1.0",
    "swagger-ui-dist": "^4.17.1",
    "yaml": "^2.2.1",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@feathersjs/errors": "^5.0.0",
    "@feathersjs/feathers": "^5.0.0",
    "@feathersjs/koa": "^5.0.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "conventional-changelog-angular": "^5.0.13",
    "cors": "^2.8.5",
    "eslint": "^8.35.0",
    "knex": "^2.4.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "release-it": "^15.7.0",
    "sqlite3": "^5.1.4",
    "supertest": "^6.3.3",
    "winston": "^3.8.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "semistandard --fix",
    "mocha": "mocha --recursive test/**/*.test.js",
    "build": "babel src -d lib",
    "test": "printf '\\33c\\e[3J' && npm run lint && npm run coverage",
    "test:dev": "printf '\\33c\\e[3J' && npm run lint && npm run mocha -- --watch --esm --watch-files ./test/**/*,./src/**",
    "coverage": "nyc npm run mocha",
    "release": "release-it --dry-run --ci --config .release-it.js",
    "___install": "npm run add:build && npm run add:merge && npm run add:generate && npm run add:validate && npm run add:mock && npm run add:swagger"
  },
  "semistandard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "examples/",
      "node_modules",
      "lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesvillarrubia/feathers-rpc.git"
  },
  "keywords": [
    "feathers",
    "rpc",
    "feathers-plugin"
  ],
  "author": {
    "name": "James Villarrubia",
    "email": "james.villarrubia@gmail.com"
  },
  "contributors": [
    {
      "name": "James Villarrubia",
      "email": "james.villarrubia@gmail.com"
    },
    {
      "name": "Ben Zelinski",
      "email": "bziggz@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesvillarrubia/feathers-rpc/issues"
  },
  "homepage": "https://github.com/jamesvillarrubia/feathers-rpc#readme"
}
